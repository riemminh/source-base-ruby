    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "simplebar-react": "^3.2.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
